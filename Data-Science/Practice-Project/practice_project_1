import pandas as pd

# Import data
# USDA ERS education and unemployment data
edu = pd.read_csv('Education.csv', skiprows=3, encoding='cp1252')
unemp = pd.read_csv('Unemployment.csv', skiprows=4)

print(edu.info())
print(unemp.info())
